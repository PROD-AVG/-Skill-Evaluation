        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {	
	// Modified record with Approved status
	if(previous.status == 3) {
		// Store the last approved proficency level
		current.last_approved_proficency_level = previous.proficiency_level;
		// Update status to modified
		current.status = 2;
	} 	
	// Modified record with Modified status
	else{
		// Check if the modified matched last approved proficency level
		if(current.proficiency_level == current.last_approved_proficency_level) {
			// Update status back to approved
			current.status = 3;
		} else {
			// Do nothing
		}
	}
})(current, previous);]]></script>
