        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	/**
	** Update valid submission
	** Procedure: 
	**     1. uncheck valid on all currently valid submission of requester
	**     2. check valid on current submission, which is the last approved submission
	*/
	var gr = new GlideRecord('x_424363_ski_eva_1_submission');
	gr.addQuery('assigned_to', current.assigned_to);
	gr.addQuery('valid', true);
	gr.setValue('valid', false);
	gr.updateMultiple();

	current.setValue('valid', true);
})(current, previous);

]]></script>
